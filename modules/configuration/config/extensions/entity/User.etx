      name="PersonalData"
      value="ObfuscateDefault"/>
  </column-override>
  <typekey-override
    name="ExperienceLevel">
    <tag
      name="PersonalData"
      value="ObfuscateDefault"/>
  </typekey-override>

  <!-- <column
    xmlns=""
    default="12"
    desc="integer extension; default value of '12'"
    name="IntegerExt"
    nullok="true"
    type="integer"/> -->
  <typekey
    desc="High level claim type (for example, Auto or Property)."
    name="LossType"
    nullok="true"
    typelist="LossType">
    <tag
      name="PersonalData"
      value="ObfuscateDefault"/>
  </typekey>
  <typekey
    desc="High level policy type (for example, Auto or Property)."
    name="PolicyType"
    nullok="true"
    typelist="PolicyType">
    <tag
      name="PersonalData"
      value="ObfuscateDefault"/>
  </typekey>
  <typekey
    desc="Default quick claim values categorized by LossType."
    name="QuickClaim"
    nullok="true"
    typelist="QuickClaimDefault">
    <tag
      name="PersonalData"
      value="ObfuscateDefault"/>
    <keyfilters>
      <keyfilter
        name="LossType"/>
    </keyfilters>
  </typekey>
</extension>