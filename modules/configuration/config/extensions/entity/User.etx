<?xml version="1.0"?>
<extension
  xmlns="http://guidewire.com/datamodel"
  entityName="User">
  <implementsInterface
    iface="gw.api.obfuscation.Obfuscator"
    impl="gw.personaldata.obfuscation.UserObfuscator"/>
  <column
    name="OffsetStatsUpdateTime"
    nullok="true"
    type="datetime">
    <tag
      name="PersonalData"
      value="ObfuscateDefault"/>
  </column>
  <column
    name="NewlyAssignedActivities"
    nullok="true"
