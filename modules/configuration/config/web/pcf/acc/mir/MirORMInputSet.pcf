<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <InputSet
    editable="true"
    id="MirORMInputSet">
    <Require
      name="anMIRReportable__Acc"
      type="MirReportable_Acc"/>
    <RangeInput
      editable="true"
      helpText="DisplayKey.get(&quot;Accelerator.mir.definitions.orm&quot;)"
      id="MirHasORmInput"
      label="DisplayKey.get(&quot;Accelerator.mir.label.orm&quot;)"
      required="anMIRReportable__Acc.ORMTermDate != null"
      value="anMIRReportable__Acc.HasORM"
      valueRange="new String[]{acc.mir.mirsubmitfs.dataservice.enums.TripleState.Yes.Code, acc.mir.mirsubmitfs.dataservice.enums.TripleState.No.Code, acc.mir.mirsubmitfs.dataservice.enums.TripleState.Unknown.Code}"
      valueType="java.lang.String"/>
    <DateInput
      editable="true"
      helpText="DisplayKey.get(&quot;Accelerator.mir.definitions.OrmTerm&quot;)"
      id="OrmTermDate"
      label="DisplayKey.get(&quot;Accelerator.mir.label.ormterm&quot;)"
      value="anMIRReportable__Acc.ORMTermDate"/>
  </InputSet>
</PCF>